[layer_names]

2d_physics/layer_1="pachinkoball"
2d_physics/layer_2="world"
2d_physics/layer_3="balldelcollision"
2d_physics/layer_4="balldelete"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
